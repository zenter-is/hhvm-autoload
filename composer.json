{
  "name": "hhvm/hhvm-autoload",
  "replace": {
    "facebook/hhvm-autoload": "1.*"
  },
  "type": "composer-plugin",
  "bin": [
    "bin/hh-autoload"
  ],
  "require": {
    "hhvm": "^3.28"
  },
  "require-dev": {
    "hhvm/hacktest": "^1.0",
    "facebook/fbexpect": "^2.1"
  },
  "autoload": {
    "classmap": [
      "src/Writer.php",
      "src/"
    ],
    "files": [
      "src/AutoloadMap.php",
      "src/Config.php",
      "src/TypeAssert.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "extra": {
    "class": [
      "Facebook\\AutoloadMap\\ComposerPlugin"
    ],
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
